{
  "version": 1,
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss",
      "changeDetection": "OnPush",
      "displayBlock": true,
      "skipTests": true,
      "inlineTemplate": true,
      "name": "NewComponent"
    },
    "@schematics/angular:directive": {
      "export": true,
      "name": "NewDirective"
    },
    "@schematics/angular:guard": {
      "implements": ["CanActivate", "CanActivateChild", "CanLoad", "CanDeactivate"],
      "name": "NewGuard"
    },
    "@schematics/angular:interceptor": {
      "name": "NewInterceptor"
    },
    "@schematics/angular:service": {
      "name": "NewService"
    },
    "@ngrx/schematics:store": {
      "no-flat": true
    },
    "@nrwl/angular:ngrx": {
      "barrels": true,
      "facade": true,
      "useDataPersistence": true
    }
  },
  "projects": {
    "admin-panel": {
      "projectType": "application",
      "root": "apps/admin-panel",
      "sourceRoot": "apps/admin-panel/src",
      "prefix": "erapulus",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/admin-panel",
            "index": "apps/admin-panel/src/index.html",
            "main": "apps/admin-panel/src/main.ts",
            "polyfills": "apps/admin-panel/src/polyfills.ts",
            "tsConfig": "apps/admin-panel/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/admin-panel/src/favicon.ico",
              "apps/admin-panel/src/assets",
              {
                "input": "libs/ui/assets/src/lib",
                "glob": "**/*",
                "output": "assets"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/ui/themes/src/lib/styles"]
            },
            "styles": ["apps/admin-panel/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/admin-panel/src/environments/environment.ts",
                  "with": "apps/admin-panel/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "admin-panel:build:production"
            },
            "development": {
              "browserTarget": "admin-panel:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "admin-panel:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/admin-panel/src/**/*.ts",
              "apps/admin-panel/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/admin-panel"],
          "options": {
            "jestConfig": "apps/admin-panel/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": ["type:application"],
      "implicitDependencies": ["ui-assets", "ui-themes"]
    },
    "admin-panel-e2e": {
      "root": "apps/admin-panel-e2e",
      "sourceRoot": "apps/admin-panel-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/admin-panel-e2e/cypress.json",
            "devServerTarget": "admin-panel:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "admin-panel:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/admin-panel-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": ["type:e2e"],
      "implicitDependencies": ["admin-panel"]
    },
