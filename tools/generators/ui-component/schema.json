{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "ui-component",
  "type": "object",
  "description": "Creates UI component",
  "properties": {
    "name": {
      "type": "string",
      "description": "Component name",
      "$default": {
        "$source": "argv",
        "index": 2
      },
      "x-prompt": "Component name:"
    }
  },
  "required": [
    "name"
  ]
}
