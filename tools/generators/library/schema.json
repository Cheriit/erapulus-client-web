{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "library",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "type": {
      "type": "string",
      "description": "Library type",
      "$default": {
        "$source": "argv",
        "index": 1
      }
    }
  },
  "required": ["name", "type"]
}
