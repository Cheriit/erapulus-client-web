{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "Module",
  "type": "object",
  "description": "Creates Module",
  "properties": {
    "project": {
      "type": "string",
      "description": "Project name",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "Module project name:"
    },
    "module": {
      "type": "string",
      "description": "Module name",
      "$default": {
        "$source": "argv",
        "index": 1
      },
      "x-prompt": "Module module name:"
    },
    "name": {
      "type": "string",
      "description": "Module name",
      "$default": {
        "$source": "argv",
        "index": 2
      },
      "x-prompt": "Module name:"
    },
    "routing": {
      "type": "boolean",
      "description": "Module has routing",
      "$default": {
        "$source": "argv",
        "index": 3
      },
      "x-prompt": "Does module has rooting? "
    },
    "route": {
      "type": "string",
      "description": "Route name",
      "$default": {
        "$source": "argv",
        "index": 4
      },
      "x-prompt": "Route name:"
    }
  },
  "required": [
    "project",
    "module",
    "name",
    "routing"
  ]
}
